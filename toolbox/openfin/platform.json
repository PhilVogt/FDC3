{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "stable"
    },
    "platform": {
        "uuid": "fdc3_explained_platform",
        "providerUrl": "http://localhost:10000/toolbox/openfin/provider.html",
        "autoShow": false,
        "fdc3InteropApi": "1.2",
